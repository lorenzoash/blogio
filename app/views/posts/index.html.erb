<h1>FUCK U</h1>

<%= link_to "New Post", posts_new_path %>

<table>
<tbody>
<tr>
    <th>Post Title</th>
    <th>Upvotes</th>
    <th>Date Published</th>
    </tr>
<% @posts.each do |post| %>
<tr>
    <td><%= post.title %></td>
    <td><%= post.upvotes %></td>
    <td><%= post.created_at %></td>
</tr>
<% end %>
</tbody>
</table>
